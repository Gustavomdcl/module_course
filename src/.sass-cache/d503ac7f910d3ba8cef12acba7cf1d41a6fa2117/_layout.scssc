:@template"Q// container -----------------------------

.l-container {
  max-width: $container-max-width;
  margin: 0 auto;
}

// row & cf ------------------------------

.l-row,
.cf {
  &:before,
  &:after { content: ''; display: table; }
  &:after { clear: both; }
}

// columns http://960.gs -------------------------------

//http://www.w3schools.com/cssref/css_selectors.asp
[class*=l-col] {
  @include box-sizing(border-box); // content-box = tamanho dele √© definido pelo seu conte√∫do apenas | border-box = O width ser√° a soma do conte√∫do com a borda e o padding: http://compass-style.org/examples/compass/css3/box_sizing/ , http://sergiolopes.org/css-box-sizing-border-box/
  float: left;
  padding: $col-padding;
  @include respond-to(max-width, $broken) {
    float: none;
    width: 100%;
    padding: 5px 10px;
  }
}

$column_width: percentage(1/$num_columns);
@for $i from 1 through $num_columns {
  .l-col#{$i} { //.col1 .col6
    width: $i * $column_width;
    @include respond-to(max-width, $broken) {
      width: 100%;
    }
  }
}



// modulos ----------------------------------

#site {
  padding: 10px;
  @include respond-to(max-width, $broken) {
    padding: 0px;
  }
}

#container {
  position: relative;
  width: $container-max-width;
  height: ($content-height + $header-height + $footer-height);
  margin: 0 auto;
  -webkit-border-radius: 10px;
  border-radius: 10px;
  overflow: hidden;
  border: 1px solid $color-gray-light;
  border-bottom: 3px solid $color-gray-light;
  background-color: $color-white;
  @include respond-to(max-width, $broken) {
    width: 100%;
    height: auto;
    -webkit-border-radius: 0px;
    border-radius: 0px;
    overflow: auto;
    padding: 90px 0px 80px 0px;
    border: 0px solid $color-gray-light;
    border-bottom: 0px solid $color-gray-light;
    background-color: $color-white;
  }
  .pie & {
    behavior: url(assets/css/suiting/pie.htc);
  }
}

#load-document {
  position: absolute;
  width: 100%;
  height: 100%;
  background-image: url('../img/template/loading.gif');
  background-repeat: no-repeat;
  background-position: center;
  z-index: 150;
  background-color: $color-blue;
  -webkit-border-radius: 10px;
  border-radius: 10px;
  @include respond-to(max-width, $broken) {
    position: fixed;
    top: 0px;
    left: 0px;
  }
  .pie & {
    behavior: url(assets/css/suiting/pie.htc);
  }
}

#header {
  width: 100%;
  height: $header-height;
  -webkit-border-radius: 10px 10px 0 0;
  border-radius: 10px 10px 0 0;
  background-image: url('../img/template/header.png');
  color: $color-white;
  a {
    color: $color-white;
    &:hover {
      color: $color-blue-dark;
    }
  }
  img {
    padding: 13px 0px 0px 14px;
  }
  .menu {
    position: absolute;
    bottom: 0px;
    left: 0px;
    z-index: 30;
    @include respond-to(max-width, $broken) {
      .menu:hover {
        color: $color-white;
      }
    }
  }
  @include respond-to(max-width, $broken) {
    z-index: 20;
    position: fixed;
    top: 0px;
    height: $header-mobile-height;
    -webkit-border-radius: 0px;
    border-radius: 0px;
    background-image: none;
    background-color: $color-blue;
    img {
      padding: 10px;
      width: auto;
      height: 65px;
    }
    .menu {
      position: absolute;
      bottom: 52.5px;
      right: 40px;
      left: inherit;
      font-size: 5em;
      z-index: 30;
    }
  }
  .pie & {
    behavior: url(assets/css/suiting/pie.htc);
  }
}

#menu {
  position: absolute;
  display: none;
  top: $header-height + 10;
  left: 0px;
  width: 300px;
  height: $content-height - 40;
  overflow: auto;
  padding: 10px 0px;
  z-index: 10;
  background-color: $color-blue;
  -webkit-border-radius: 0 10px 10px 0;
  border-radius: 0 10px 10px 0;
  [class*=entypo] {
    margin-right: 10px;
  }
  li {
    display: block;
    a {
      color: $color-white;
      display: block;
      padding: 10px;
      padding-left: 40px;
      &:hover {
        color: $color-blue;
        background-color: $color-white;
      }
      &.current {
        color: $color-white;
        background-color: $color-blue-dark;
      }
      &.current:hover {
        cursor: default;
        background-color: $color-blue-dark;
      }
      &.disciplina {
        padding-left: 10px;
      }
      &.modulo {
        padding-left: 20px;
      }
      &.topico, &.sumario, &.introducao, &.referencias, &.atividades {
        padding-left: 30px;
      }
    }
  }
  @include respond-to(max-width, $broken) {
    @include box-sizing(border-box);
    left: 0px;
    position: fixed;
    top: 0px;
    width: 100%;
    height: 100%;
    overflow: auto;
    -webkit-border-radius: 0px;
    border-radius: 0px;
    padding: $header-mobile-height 0px 40px 0px;
    background-color: $color-blue;
  }
  behavior: url(assets/css/suiting/pie.htc);
}

#content {
  @include box-sizing(border-box);
  width: 100%;
  height: $content-height;
  overflow: hidden;
  padding: 0px 10px;
  @include respond-to(max-width, $broken) {
    overflow: inherit;
    height: auto;
  }
}

#footer {
  width: 100%;
  height: $footer-height;
  color: $color-white;
  -webkit-border-radius: 0 0 10px 10px;
  border-radius: 0 0 10px 10px;
  background-color: $color-blue;
  a {
    color: $color-white;
    &:hover {
      color: $color-blue-dark;
    }
  }
  .main-audio {
    color: $color-orange;
    display: none;
  }
  .audio {
    display: inline-block;
    height: $footer-height;
    *zoom: 1;
    *display: inline;
    .reload {
      margin-left: 60px;
    }
    .play {
    }
    .control {
      width: 180px;
      height: $footer-height - 5;
      padding-top: 5px;
      text-align: center;
      font-size: 1.8em;
      display: inline-block;
      *zoom: 1;
      *display: inline;
      .line {
        position: relative;
        width: 100%;
        height: 10px;
        margin-bottom: 5px;
        display: inline-block;
        cursor: pointer;
        background-color: $color-white;
        .progress {
          position: absolute;
          top: 0px;
          left: 0px;
          width: 0px;
          height: 10px;
          display: block;
          overflow: hidden;
          background-color: $color-blue-dark;
        }
        .pointer {
          position: absolute;
          top: -11px;
          left: 17px;
          color: $color-blue-dark;
          cursor: pointer;
        }
      }
    }
    .display {
      width: 110px;
      height: $footer-height - 1;
      padding-top: 1px;
      text-align: center;
      display: inline-block;
      vertical-align: top;
      *zoom: 1;
      *display: inline;
    }
    @include respond-to(max-width, $broken) {
      display: none;
      position: fixed;
      width: 100%;
      bottom: 80px;
      text-align: center;
      background-color: $color-orange;
      .reload {
        margin-left: 0px;
      }
      .display {
        display: none;
      }
    }
  }
  .back {
    margin-left: 305px;
  }
  .position {
    width: 110px;
    height: $footer-height;
    text-align: center;
    display: inline-block;
    vertical-align: top;
    *zoom: 1;
    *display: inline;
  }
  .error {
    font-size: 1.8em;
    visibility: hidden;
  }
  @include respond-to(max-width, $broken) {
    height: 80px;
    position: fixed;
    bottom: 0px;
    z-index: 5;
    -webkit-border-radius: 0px;
    border-radius: 0px;
    .main-audio {
      position: absolute;
      left: 50%;
      display: block;
      font-size: 4em;
      margin-left: -32px;
    }
    .position {
      display: none;
    }
    .back {
      position: absolute;
      left: 5px;
      margin-left: 0px;
      font-size: 4em;
    }
    .foward {
      position: absolute;
      right: 35px;
      font-size: 4em;
    }
  }
  .pie & {
    behavior: url(assets/css/suiting/pie.htc);
  }
}
.back-session {
  text-align: center;
  padding-bottom: 120px;
  display: inherit;
}
