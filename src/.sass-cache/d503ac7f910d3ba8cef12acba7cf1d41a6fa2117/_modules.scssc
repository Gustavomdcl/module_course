.internal {

	//Estilos de Par√°grafo =============================================

	font-family: 'Open Sans', sans-serif;
	font-size: 15px;
	.color-white {
		color: $color-white;
	}
	img {
		@include box-sizing(border-box);
		max-width: 100%;
		margin: 5px;
		&.full {
			@include box-sizing(border-box);
			width: 100%;
		}
	}
	.bloco {
		@include box-sizing(border-box);
		padding: 30px;
		-webkit-box-shadow: 0 0 5px 0 #545454;
		box-shadow: 0 0 5px 0 #545454;
		-webkit-border-radius: 15px;
		border-radius: 15px;
		.pie & {
			behavior: url(assets/css/suiting/pie.htc);
		}
	}
	img.bloco {
		padding: 2px;
	}
	p, ul {
		line-height: 1.5em;
		font-size: 1em;
		@include respond-to(max-width, $broken) {
			font-size: 1em;
		}
	}
	ul {
		padding-left: 50px;
		li {
			list-style: circle;
			list-style-image: url('../img/template/simbolo-little.png');
		}
		&.inverted {
			li {
				list-style: circle;
				list-style-image: url("../img/template/simbolo-little-white.png");
			}
		}
	}
	a, .modal {
		color: $color-orange;
		text-decoration: underline;
	}
	table {
		width: 100%;
		color: $color-white;
		tr {
			td {
